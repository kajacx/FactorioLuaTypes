{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true
  },
  "files": [
    "main.ts"
  ]
}
